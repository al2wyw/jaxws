<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" targetNamespace="http://www.myws.com" xmlns:tns="http://www.myws.com" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:element name="name" type="tns:name"/>

  <xs:element name="nameResponse" type="tns:nameResponse"/>

  <xs:complexType name="name">
    <xs:sequence>
      <xs:element name="customer" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="nameResponse">
    <xs:sequence>
      <xs:element name="outcome" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
</xs:schema>

